{
    "name": "sdms",
    "version": "4.0.0",
    "license": "Apache-2.0",
    "repository": {
        "url": "https://community.opengroup.org/osdu/platform/domain-data-mgmt-services/seismic/seismic-dms-suite/seismic-store-service/-/tree/master/app/sdms-v4"
    },
    "description": "Seismic Domain Data Management APIs to store and manage strongly typed seismic datasets.",
    "scripts": {
        "build": "tsc",
        "copy-static-files": "copyfiles --error --up 1 ./docs/openapi.yaml ./docs/schemas/**/*.json ./docs/schemas/examples/**/*.json ./docs/schemas/generated/**/*.json ./dist/docs",
        "copy-types-hpropagate": "copyfiles --error --up 2 ./typescript/types/hpropagate/**/*.d.ts ./node_modules/@types",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "postbuild": "npm run copy-static-files",
        "postinstall": "npm run copy-types-hpropagate",
        "start-service": "node --trace-warnings --trace-uncaught ./dist/server/server-start.js"
    },
    "dependencies": {
        "@azure/core-auth": "^1.3.2",
        "@azure/core-rest-pipeline": "^1.8.1",
        "@azure/identity": "^2.0.4",
        "@azure/keyvault-secrets": "^4.4.0",
        "@azure/storage-blob": "^12.10.0",
        "@types/qs": "^6.9.7",
        "axios": "^0.27.2",
        "copyfiles": "^2.4.1",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "hpropagate": "^1.0.1",
        "ioredis": "^5.0.5",
        "js-yaml": "^4.1.0",
        "json-refs": "^3.0.15",
        "jsonschema": "^1.4.1",
        "node-cache": "^5.1.2",
        "qs": "^6.10.3",
        "swagger-ui-express": "^4.4.0",
        "typescript": "^4.4.4",
        "xss-filters": "^1.2.7"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/js-yaml": "^4.0.5",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/xss-filters": "0.0.27",
        "@typescript-eslint/parser": "^5.26.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.6.2"
    }
}
