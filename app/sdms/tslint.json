{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "quotemark": [ true, "single" ],
        "max-classes-per-file":false,
        "member-ordering": false,
        "no-string-literal": false,
        "no-floating-promises": true
    },
    "jsRules": { }
}
